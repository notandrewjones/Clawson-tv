{
  "buildCommand": "npm run build",
  "outputDirectory": "public",
  "installCommand": "npm install",
  "framework": null,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Content-Security-Policy", "value": "frame-ancestors 'self' https://*.wix.com https://*.wixsite.com https://*.editorx.com" },
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ]
}